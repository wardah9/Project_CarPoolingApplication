<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#65c3ce</item>
        <item name="colorPrimaryDark">#1c929f</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Button" parent="Theme.AppCompat.Light">
        <item name="colorControlHighlight">@android:color/white</item>
        <item name="colorButtonNormal">#cc62b3a0</item>
        <item name="borderlessButtonStyle" width="9dp">@android:color/white</item>
    </style>

    <style name="MyButton" parent="Theme.AppCompat.Light">
        <item name="colorControlHighlight">#18a7a9</item>
    </style>
    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@android:color/holo_orange_dark</item>
        <item name="colorControlActivated">@android:color/holo_green_light</item>
    </style>

</resources>
